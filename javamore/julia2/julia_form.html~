ra:<input size="4" name="redAjust" value="0" type="text">&nbsp;
ga:<input size="4" name="greenAjust" value="0" type="text">&nbsp;
ba:<input size="4" name="blueAjust" value="0" type="text">&nbsp;

<input value="julia set" 
onclick="document.julia.setC1(this.form.c1.value);
document.julia.setC2(this.form.c2.value);
document.julia.setScale(this.form.scale.value);

document.julia.setREDajust(this.form.redAjust.value);
document.julia.setGREENajust(this.form.greenAjust.value);
document.julia.setBLUEajust(this.form.blueAjust.value);"


type="button">
</form>

<applet  code="Julia3.class" height="1000" width="1000">
