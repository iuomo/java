class Delay {  

  void delay(long i) {  
	  //long delay = i;
	  long t0, t1;
	  t0 =  System.currentTimeMillis();
	  do{
	  	t1 = System.currentTimeMillis();
	  }while (t1 - t0 < i);	
  	}    
}



