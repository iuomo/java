class RandomIndex {
	public static void main (String args[]){
		int i;
		int rv;//random value
		int ra[] = new int[52];//random array
	
		for (i = 0; i < 52; i++) ra[i] = 0;
		
	for (i = 0; i < 52; i++) {
			rv = (int)(Math.random() * 52);
			System.out.print(rv+" ");
			ra[rv] = 1;
			if (i % 5 == 0) System.out.println();
			}
			

	System.out.println();
	for (i = 0; i < 52; i++) {System.out.print(ra[i]);
			if (i % 5 == 0) System.out.println();
	}
		System.out.println();	
	}
}
